{
  "name": "devanalytics",
  "displayName": "DevAnalytics",
  "description": "Privacy-friendly local analytics with a React dashboard.",
  "version": "0.4.3",
  "publisher": "RaulAlazovi069",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaulAlazovi069/dev-analytics"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:devanalytics.showDashboard",
    "onCommand:devanalytics.exportData",
    "onCommand:devanalytics.reset"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devanalytics.showDashboard",
        "title": "DevAnalytics: Show Dashboard"
      },
      {
        "command": "devanalytics.exportData",
        "title": "DevAnalytics: Export Data (JSON)"
      },
      {
        "command": "devanalytics.reset",
        "title": "DevAnalytics: Reset Data"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -p . -w",
    "ui:build": "cd webview-ui && npm install && npm run build",
    "ui:dev": "cd webview-ui && npm run dev",
    "build": "npm run ui:build && npm run compile",
    "package": "npm run build && npx vsce package"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/vscode": "^1.105.0",
    "typescript": "^5.6.3",
    "vsce": "^2.15.0"
  }
}
